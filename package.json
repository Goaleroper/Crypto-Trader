    "husky": "^9.0.11",
    "postcss": "^8.4.27",
    "tailwindcss": "^3.3.3"
  },
  "husky": {
    "hooks": {
      "post-merge": "git-branch-is master && npm version --patch || npm --no-git-tag-version version patch",
      "pre-push": "git-branch-is master && npm version patch && git push --follow-tags || (npm --no-git-tag-version version patch && git add package.json && git commit -m 'Bump version' && git push)"
    }
  }
}
